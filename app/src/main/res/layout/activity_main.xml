                        android:layout_gravity="center"
                        android:orientation="horizontal">

                        <RadioButton
                            android:id="@+id/main_prioRB_D_min"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/text_min"
                            android:textColor="@android:color/white" />

                        <RadioButton
                            android:id="@+id/main_prioRB_D_low"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/text_low"
                            android:textColor="@android:color/white" />

                        <RadioButton
                            android:id="@+id/main_prioRB_D_def"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/text_default"
                            android:textColor="@android:color/white" />

                        <RadioButton
                            android:id="@+id/main_prioRB_D_high"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/text_high"
                            android:textColor="@android:color/white" />

                        <RadioButton
                            android:id="@+id/main_prioRB_D_max"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/text_max"
                            android:textColor="@android:color/white" />
                    </RadioGroup>
                </HorizontalScrollView>
            </LinearLayout>

            <Switch
                android:id="@+id/settingsScreen_showBCV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="false"
                android:paddingTop="@dimen/fifteen"
                android:text="@string/text_showBCV"
                android:textColor="@android:color/white"
                android:textSize="@dimen/main_textSmall" />

            <Switch
                android:id="@+id/settingsScreen_showNotification"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="false"
                android:paddingTop="@dimen/fifteen"
                android:text="@string/text_showNotification"
                android:textColor="@android:color/white"
                android:textSize="@dimen/main_textSmall" />

            <Switch
                android:id="@+id/settingsScreen_showHeadphonePopup"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="false"
                android:paddingTop="@dimen/fifteen"
                android:text="@string/text_showHeadphonesPopup"
                android:textColor="@android:color/white"
                android:textSize="@dimen/main_textSmall" />

            <Switch
                android:id="@+id/settingsScreen_adderall_resetAtMidnight"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="false"
                android:paddingTop="@dimen/fifteen"
                android:text="@string/text_resetAtMidnight"
                android:textColor="@android:color/white"
                android:textSize="@dimen/main_textSmall" />


            <Switch
                android:id="@+id/settingsScreen_showScreamingSun"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/black"
                android:checked="false"
                android:paddingTop="@dimen/fifteen"
                android:text="@string/text_showRickAndMortyPopups"
                android:textAlignment="center"
                android:textColor="@android:color/white"
                android:textSize="@dimen/main_textSmall" />
        </LinearLayout>
    </ScrollView>
</RelativeLayout>
